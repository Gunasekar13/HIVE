ðŸ‘‰#create table

create table emp(
emp_no int,
ename string,
job string,
mgr int,
hiredate string,
sal int,
comm int,
deptno int
)
row format delimited
fields terminated by ','
tblproperties('skip.header.line.count'='1')
;

ðŸ‘‰#load data from loacl path to table

load data local inpath '/config/workspace/practice/employee.txt' into table emp;

ðŸ‘‰#create a temporary table load empno,ename,sal from above table

create table temp_table(
EMPNO int,
ENAME string,
SALARY int
)
row format delimited
fields terminated by ','
;

insert table temp_table select emp_no,ename,sal from emp;

ðŸ‘‰#insert overwrite the temporary table load empno,ename,sal from above emp table who have sal is more than 1000

insert overwrite table temp_table select emp_no,ename,sal from emp where sal>1000;
